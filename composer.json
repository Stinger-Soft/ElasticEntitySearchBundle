{
	"name" : "stinger-soft/elastic-entity-search-bundle",
	"type" : "symfony-bundle",
	"description" : "Elastic Layer to index and search entities",
	"keywords" : [
		"Search"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\ElasticEntitySearchBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : ">=7.2",
		"stinger-soft/entity-search-bundle" : "~1.1@dev",
		"knplabs/knp-paginator-bundle" : "^5.0",
		"stinger-soft/php-commons" : "^1.0",
		"elasticsearch/elasticsearch" : "^6.0",
		"ruflin/elastica" : "^6.0",
		"monolog/monolog" : "~1.19",
    },
	"require-dev" : {
		"phpunit/phpunit" : "~7.3.5",
		"php-coveralls/php-coveralls" : "~2.1.0",
		"doctrine/orm" : ">=2.5.0",
		"doctrine/common" : ">=2.5.0",
		"symfony/validator" : "^3.4|^4.0",
		"symfony/phpunit-bridge" : "^3.4|^4.0"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.1.x-dev",
			"dev-develop" : "1.0.x-dev"
		}
	}
}
